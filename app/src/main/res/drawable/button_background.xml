<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true"></item>
<shape
    android:shape="rectangle">
    <gradient
        android:startColor="#1C7B10"
        android:endColor="#33DE1D"
        android:angle="90"
        android:type="linear"/>
    <padding
        android:top="7dp"
        android:bottom="7dp"
        android:left="7dp"
        android:right="7dp"/>
    <corners
        android:radius="15dp"/>

    <stroke
        android:width="2dp"/>
</shape>
</selector>